<!DOCTYPE html>
<html>

<head>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.5.4/socket.io.js"></script>
  <style>
    body {
      background-color: transparent;
    }
  </style>
</head>

<body>
  <script>
    const socket = io();

    socket.on("data", (data) => {
      const audioUrl = data;
      const audioApi = new Audio(audioUrl);
      audioApi.play();
      console.log("playing")
    });
  </script>
</body>

</html>
